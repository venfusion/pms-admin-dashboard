    "@mui/material": "^6.4.0",
    "@tanstack/react-query": "^5.64.1",
    "@tanstack/react-query-devtools": "^5.64.1",
    "@toolpad/core": "^0.13.0",
    "dayjs": "^1.11.13",
    "ky": "^1.7.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^5.0.0",
    "react-helmet": "^6.1.0",
